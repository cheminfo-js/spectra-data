var Data = require('..');
var FS = require('fs');
var lib = require("ml-fft");
var FFT = lib.FFT;
function createSpectraData(filename, label, data) {
    var spectrum = Data.NMR.fromJcamp(
        FS.readFileSync(__dirname + filename).toString()
    );
    return spectrum;
};


var nH = 8;
var spectrum=createSpectraData("/data/FID.dx");


var y;
for(var i=0;i<67;i++){
    y = spectrum.sd.spectra[0].data[0].y.splice(0,1);
    spectrum.sd.spectra[0].data[0].y.push(y);
    y = spectrum.sd.spectra[1].data[0].y.splice(0,1);
    spectrum.sd.spectra[1].data[0].y.push(y);
}

spectrum.apodization("gaussian").zeroFilling(1024,0).brukerSpectra().fourierTransform().postFourierTransform();
spectrum.phaseCorrection(10,0.1);
console.log(spectrum.getVector(spectrum.getFirstX(),spectrum.getLastX(),4*1024));


















